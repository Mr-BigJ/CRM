  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="crm.workbench.dao.ClueActivityRelationDao">
    <delete id="delete">
        delete from tbl_clue_activity_relation where clueId=#{clueId}
    </delete>
	
	<select id="getListByculeId" resultType="ClueActivityRelation">
